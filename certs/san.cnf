[ req ]
default_bits       = 4096
prompt             = no
default_md         = sha256
req_extensions     = v3_req
distinguished_name = dn

[ dn ]
C  = SE
ST = -
L  = -
O  = ThesisLab
CN = registry

[ v3_req ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = registry
DNS.2 = localhost
DNS.3 = host.docker.internal
IP.1  = 127.0.0.1
wsl